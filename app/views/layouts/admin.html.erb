<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <title>Airbo</title>
    <%= stylesheet_link_tag "app-admin" %>
    <%= javascript_include_tag 'app-admin' %>

    <%= csrf_meta_tag %>
    <%= mobvious_javascript %>
  </head>
  <body class="<%= body_class %>">

    <%= render 'shared/admin_flashes' %>
    <%= render 'shared/header', :show_play_box => false %>
    <%=render "admin/site_admin_nav"%>
    <%= render 'shared/offcanvas_menu_signedin' %>
      <%= yield %>
  

    <%= render :partial => 'shared/modals/modals' %>
    <%= render :partial => 'shared/javascript' %><!-- This is where 'yield :javascript' takes place -->
  </body>
</html>
