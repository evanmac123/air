<!doctype html>
<html class="no-js" lang="en">
  <head>
    <meta charset="utf-8" />
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Airbo</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <%= content_for :meta %>
    <%= stylesheet_link_tag "app-admin" %>
    <%= javascript_include_tag 'app-admin' %>

    <%= csrf_meta_tag %>
    <%= mobvious_javascript %>
    <%= content_for :head %>
  </head>
  <body class="<%= body_class %>">

    <%= render 'shared/admin/header' %>
    <%=render "admin/site_admin_nav"%>
    <div id="admin-layout-wrapper" class="toggled">
      <%= render 'shared/simple_flashes' %>
      <%= render 'shared/offcanvas_menu_signedin' %>
        <%= yield %>


      <div class="modals"></div>
    </div>
    <%= render :partial => 'shared/javascript' %>
  </body>
</html>
