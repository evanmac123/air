<!doctype html>
<html class="no-js" lang="en">
  <head>
    <meta charset="utf-8" />
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Airbo</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <%= content_for :meta %>
    <%= csrf_meta_tag %>

    <%= javascript_include_tag 'app-admin' %>

    <%= render 'shared/integrations/full_story' %>

    <%= content_for :head %>
  </head>
  <body class="<%= body_class %> client_admin_main" style="-webkit-overflow-scrolling: touch; overflow:visible; ">
    <main class="main">
      <div class="core">
        <div class="content">
          <%= yield %>
        </div>
        <div id="footer">
          <%= render 'shared/copyright' %>
        </div>
      </div>
    </main>
  </body>
</html>
