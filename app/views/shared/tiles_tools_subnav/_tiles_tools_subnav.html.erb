<% subnav = TilesToolsSubnavPresenter.new(current_user) %>
<% if subnav.show_subnav? %>
  <ul class="<%= subnav.side_nav_class %>" id="<%= subnav.side_nav_id %>">
    <% subnav.items_with_corrected_params do |item_params| %>
      <%= render partial: "shared/tiles_tools_subnav/subnav_item", locals: item_params %>
    <% end %>
  </ul>
<% end %>
