<% tiles.each do |tile| %>
  <% if display_tile_for_search(tile) %>
    <% present(tile, presenter, { is_ie: browser.ie?, referrer: :contextual_tiles, from_search: true, completed: tile_completed?(tile, current_user.end_user?) }) do |presenter| %>
      <%= render presenter.partial, presenter: presenter, tile: tile %>
    <% end %>
  <% end %>
<% end %>
