<section class="manage_section contextual_tiles_container <%= container %>" style="margin-top:30px;">
  <div class="row">
    <div class="large-12 columns">
      <div class="<%= container %>" data-page="<%= 1 %>" data-count="<%= tiles.count %>">
        <h4 class="tile-container-section"><%= section.capitalize %></h4>
        <%= render "shared/tiles/contextual_tiles", { presenter: presenter, tiles: tiles } %>
      </div>
    </div>
  </div>
</section>
