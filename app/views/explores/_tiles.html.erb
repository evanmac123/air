<% tiles.each do |tile| %>
  <% present( tile, SingleAdminTilePresenter, {public_slug: params[:public_slug],  is_ie: browser.ie?, completed: false, page_type: :explore, tile_ids: tile_ids, section: section }) do |presenter| %>
    <% cache [__FILE__, 'v1.pwd', 'single tile', presenter.cache_key] do %>
      <%= render 'client_admin/tiles/manage_tiles/single_tile', presenter: presenter %>
    <% end %>
  <% end %>
<% end %>
