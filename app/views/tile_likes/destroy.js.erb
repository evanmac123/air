<%if params[:path] == :via_explore_page_thumbnail%>
  $('body.explores').find('.tile_liked.tile-<%=@tile.id%>').hide();
  $('body.explores').find('.tile_not_liked.tile-<%=@tile.id%>').show();
  $('body.explores').find('#tile_likes_text').html('<%=show_unliked_tile(@tile)%>')
<%else%>
  $('body.tile_previews').find('.not_like_button.tile-<%=@tile.id%>').show();
  $('body.tile_previews').find('.like_button.tile-<%=@tile.id%>').hide();
  $('body.tile_previews').find('#tile_likes_text').html('<%=show_tile_likes(@tile, current_user)%>')
<%end%>
