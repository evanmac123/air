<%= render 'section', section_id: 'creation', header_text: "Draft", tooltip_params: {id: "info_draft_tile"}, wrapper_id: 'draft', tiles: @creation_placeholder, type: :user_draft %>

<%= render 'section', section_id: "submitted_tiles", header_text: "Submitted", tooltip_params: {id: "info_submitted_tile", data: {tooltip: true}, title: "Tiles you have submitted. If you have questions, contact us at support@airbo.com."}, wrapper_id: "submitted", tiles: @submitted_tiles, type: :user_submitted %>

<%= render 'section', section_id: "accepted_tiles", header_text: "Accepted", tooltip_params: {id: "info_accepted_tile"}, wrapper_id: "accepted", tiles: @accepted_tiles, type: :draft %>

<%= render 'section', section_id: "posted_tiles", header_text: "Posted", tooltip_params: {id: "info_posted_tile"}, wrapper_id: "posted", tiles: @posted_tiles, type: :active %>

<%= render 'section', section_id: "archived_tiles", header_text: "Archived", tooltip_params: {id: "info_archived_tile"}, wrapper_id: "archived", tiles: @archived_tiles, type: :archived %>

<%= render 'shared/tiles/poll_for_thumbnails' %>
<%= render "shared/header/submit_tile_modal" %>
