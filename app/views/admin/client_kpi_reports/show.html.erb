<div class="row">
  <div class="columns small-12">
    <div class="row">
      <div class="panel">
        <h3>Total Paid Client Admin</h3>
        <h3>By month:</h3>
        <%= render 'airbo_totals_table', data: Reporting::AirboTotals.get_total_paid_client_admins_by_month, display: :integer  %>

        <h3>By week:</h3>
        <%= render 'airbo_totals_table', data: Reporting::AirboTotals.get_total_paid_client_admins_by_week, display: :integer %>

      </div>
    </div>
    <div class="row">
      <div class="panel">
        <h3>Total Paid Organizations</h3>
        <h3>By month:</h3>
        <%= render 'airbo_totals_table', data: Reporting::AirboTotals.get_total_paid_organizations_by_month, display: :integer %>

        <h3>By week:</h3>
        <%= render 'airbo_totals_table', data: Reporting::AirboTotals.get_total_paid_organizations_by_week, display: :integer %>
      </div>
    </div>
    <div class="row">
      <div class="panel">
        <h3>Percent Eligible Population Joined</h3>
        <%= render 'airbo_totals_table', data: Reporting::AirboTotals.get_percent_of_eligible_population_joined, display: :percent %>
      </div>
    </div>
  </div>
</div>
