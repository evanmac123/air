<section style="background-color: #d9dfe9" style="max-width:46.8rem; margin:auto;">
<div class="row" >
  <div class="columns small-8 small-centered">
  <div class="breadcrumb" style="padding-top:30px">
    <%=link_to "Content Library", client_admin_stock_boards_path %> > <%=link_to @tile.demo.name, stock_board_path(library_slug: @tile.demo.public_slug) %> > <span><%=@tile.headline%></span> 
  </div>
  </div>
</div>
</section>
<%= render partial: 'shared/tiles/user_progress', locals: { raffle: current_user.demo.raffle } %>
<div class="container row">
  <div class="viewer">
    <%= link_to "", client_admin_stock_tile_path(@prev), :id => :prev  %>
    <div id="slideshow">
      <%= render 'tiles/full_size_tile', :tile => @tile, :current_tiles => @current_tiles %>
    </div>
    <%= link_to "", client_admin_stock_tile_path(@next), :id => :next %>
  </div>
</div>

