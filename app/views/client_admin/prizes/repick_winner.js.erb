$("#user_<%= j @user.id.to_s %>")
  .attr("id", "user_<%= j @new_user.id.to_s %>")
  .find(".winner_name").text("<%= j @new_user.name.to_s %>").end()
  .find(".winner_chances").text("<%= j @new_user.tickets.to_s %>").end()
  .find(".winner_email").text("<%= j @new_user.email.to_s %>").end()
  .find(".winner_delete").html("<%= j link_to 'Delete', delete_winner_client_admin_prizes_path(@new_user.id), remote: true, method: :delete  %>").end()
  .find(".winner_repick").html("<%= j link_to 'Re-Pick', repick_winner_client_admin_prizes_path(@new_user.id), remote: true %>").end()
