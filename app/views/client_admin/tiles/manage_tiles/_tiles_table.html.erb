<% tiles.each do |tile| %>
  <% if tile.is_placeholder? %>
    <%= render "client_admin/tiles/manage_tiles/single_#{tile.class.to_s.underscore}", placeholder: tile %>
  <% else %>
    <% present(tile, SingleAdminTilePresenter, {as_admin: @is_client_admin_action,is_ie: browser.ie?}) do |presenter| %>
      <% cache [__FILE__, 'v1.pwd', 'single tile', presenter.cache_key] do %>
        <%= render 'client_admin/tiles/manage_tiles/single_tile', presenter: presenter %>
      <% end %>
    <% end %>
  <% end %>
<% end %>
