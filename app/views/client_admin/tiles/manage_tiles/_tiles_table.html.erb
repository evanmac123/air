<% tiles.each do |tile| %>
  <% if tile.is_placeholder? %>
    <%= render "client_admin/tiles/manage_tiles/single_#{tile.class.to_s.underscore}", placeholder: tile %>
  <% else %>
    <% present(tile, SingleAdminTilePresenter, {is_ie: browser.ie?}) do |presenter| %>
      <%= render partial: 'client_admin/tiles/manage_tiles/client_admin_tile_thumbnail', locals:{ presenter: presenter } %>
    <% end %>
  <% end %>
<% end %>
