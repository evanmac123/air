- display_no_tiles_section = (@archive_tiles.empty? || @archive_tiles.all?(&:is_placeholder?)) ? "block" : "none"
%div.no_tiles_section{style: "display:#{display_no_tiles_section};"}
  - tile = TileOddRowPlaceholder.new
  - 4.times do
    = render "client_admin/tiles/manage_tiles/single_#{tile.class.to_s.underscore}", tile: tile

= render partial: 'client_admin/tiles/manage_tiles/tiles_table', locals: {tiles: @archive_tiles, type: :archive}
