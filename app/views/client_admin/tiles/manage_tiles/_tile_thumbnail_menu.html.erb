<ul class="tile_thumbnail_menu">
  <li class="duplicate_button">
    <%= link_to duplicate_client_admin_tile_path(presenter, page: 'Edit'), class: "duplicate_tile", data: {"tile-id" => presenter.tile_id} do %>
      <%= fa_icon 'copy' %>
      <span>Copy</span>
    <% end %>
  </li>

  <li class="destroy_button">
    <%= link_to client_admin_tile_path(presenter, page: 'Edit'), class: "delete_tile", data: {"tile-id" => presenter.tile_id}  do %>
      <%= fa_icon 'trash-o' %>
      <span>Delete</span>
    <% end %>
  </li>
</ul>
