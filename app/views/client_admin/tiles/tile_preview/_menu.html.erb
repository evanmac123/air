<% show_client_admin_menu ||= false %>
<%  if show_client_admin_menu %>
  <% if tile.suggested? %>
    <%= render partial: "client_admin/tiles/tile_preview/suggested_menu", locals:{tile: tile} %>
  <% else %>
    <%= render partial: "client_admin/tiles/tile_preview/ca_created_menu", locals:{tile: tile, tooltip_class: (tile.is_fully_assembled? ? "tipsy" : "disabled")} %>
  <% end %>
<%end %>



