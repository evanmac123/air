<section class="<%= draftSectionClass %> manage_tiles compressed_section" id="draft_tiles">
  <%= render partial: 'client_admin/tiles/manage_tiles/draft' %>
</section>
<%= render partial: 'client_admin/tiles/manage_tiles/show_all_draft' %>

<div class="completed_tiles_containment">
  <section class="manage_tiles" id="active_tiles">
    <%= render partial: 'client_admin/tiles/manage_tiles/active' %>
  </section>
  <section class="manage_tiles" id="archived_tiles">
    <%= render partial: 'client_admin/tiles/manage_tiles/archive' %>
  </section>
  <%= render partial: 'client_admin/tiles/manage_tiles/show_all_archive' %>
</div>

<% if params[:from_onboarding] %>
  <%= render partial: 'client_admin/partials/from_onboarding_modal' %>
<% end %>
