<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
  <%= render partial: 'explore_digest_mailer/head' %>
  <body bgcolor="#ffffff">
    <table bgcolor="#ffffff" width="100%" cellpadding="0" cellspacing="0">
      <tr>
        <td align="center">
          <%= render partial: "explore_digest_mailer/header" %>
          <table bgcolor="#ffffff" align="center" cellpadding="0" cellspacing="0">
            <tr>
              <td width="600" align="center">
                <%= render partial: 'explore_digest_mailer/title' %>
                <%= render partial: 'explore_digest_mailer/tiles' %>
              </td>
            </tr>
          </table>
          <%= render partial: 'explore_digest_mailer/forward' %>
          <%= render partial: 'explore_digest_mailer/explore' %>
          <%= render partial: "mailer/footer", locals: { ie_link: @presenter.general_site_url } %>
        </td>
      </tr>
    </table>
  </body>
</html>
